{
  "name": "@mcph/beam-interactive-node2",
  "version": "0.9.0",
  "description": "A NodeJS and Browser compatible client for Beam.pro's interactive 2 Protocol",
  "contributors": [
      {
          "name": "Richard Fox",
          "email": "fox@xbox.com"
      },
      {
          "name": "Connor Peet",
          "email": "connor@xbox.com"
      }
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build:ts": "rimraf lib dist && tsc",
    "build:webpack": "rimraf dist && webpack && webpack --config webpack.config.min.js",
    "build:examples": "rimraf examples/*.js && cd examples && tsc",
    "build": "npm run build:ts && npm run build:webpack && npm run build:examples",
    "test": "mocha --compilers ts:ts-node/register \"src/**/*.spec.ts\"",
    "prepublish": "npm run build",
    "lint": "tslint -c tslint.json --project tsconfig.json \"src/**/*.ts\" \"test/**/*.ts\"",
    "docs": "typedoc --out docs/ --mode file --excludeNotExported -tsconfig tsconfig.json --target ES6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WatchBeam/beam-interactive-2.git"
  },
  "keywords": [
    "Interactive",
    "Beam"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WatchBeam/beam-interactive-2/issues"
  },
  "homepage": "https://github.com/WatchBeam/beam-interactive-2#readme",
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/deepmerge": "^1.3.0",
    "@types/faker": "^4.1.0",
    "@types/mocha": "^2.2.39",
    "@types/node": "^6.0.45",
    "@types/node-fetch": "^1.6.7",
    "@types/sinon": "^1.16.34",
    "@types/sinon-chai": "^2.7.27",
    "@types/ws": "0.0.38",
    "awesome-typescript-loader": "^3.0.4-rc.2",
    "chai": "^3.5.0",
    "fetch-cookie": "^0.4.1",
    "mocha": "^3.0.2",
    "node-fetch": "^1.6.3",
    "rimraf": "^2.6.0",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "ts-node": "^2.0.0",
    "tslint": "^4.3.1",
    "tslint-microsoft-contrib": "4.0.0",
    "typedoc": "^0.5.7",
    "typescript": "^2.1.6",
    "webpack": "^2.2.1",
    "webpack-bundle-analyzer": "^2.3.0",
    "ws": "^1.1.1",
    "faker": "^4.1.0"
  },
  "dependencies": {
    "deepmerge": "^1.3.2"
  }
}
